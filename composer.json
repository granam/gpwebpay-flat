{
  "name": "granam/gpwebpay-flat",
  "type": "library",
  "description": "Parser of GP WepPay report in FLAT format",
  "keywords": [
    "gp",
    "gp webpay",
    "webpay",
    "pay",
    "flat",
    "report",
    "parser"
  ],
  "homepage": "https://github.com/jaroslavtyc/gpwebpay-flat",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Jaroslav TÃ½c",
      "email": "mail@jaroslavtyc.com"
    }
  ],
  "support": {
    "email": "mail@jaroslavtyc.com",
    "issues": "https://github.com/jaroslavtyc/gpwebpay-flat/issues"
  },
  "autoload": {
    "psr-4": {
      "Granam\\GpWebPay\\Flat\\": "GpWebPayFlat"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Granam\\Tests\\GpWebPay\\Flat\\": "tests/GpWebPayFlat",
      "Granam\\Tests\\Mail\\Download\\": "vendor/granam/imap-download/tests/ImapDownload",
      "Granam\\Tests\\ExceptionsHierarchy\\": "vendor/granam/exceptions-hierarchy/tests/ExceptionsHierarchy"
    }
  },
  "require": {
    "php": ">=7.3",
    "granam/strict-object": "^3.0",
    "granam/imap-download": "^1.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "~9.0",
    "mockery/mockery": "~1.0",
    "granam/exceptions-hierarchy": "~5.0",
    "granam/test-with-mockery": "^2.0"
  },
  "suggest": {
    "ext-imap": "Needed to fetch FLAT file directly from an email via IMAP protocol"
  }
}
